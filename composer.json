{
    "name": "yiizh/yiizh.com",
    "description": "The source code of YiiZH.COM.",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.10",
        "yiizh/yii2-adminlte": "*",
        "yiizh/yii2-fontawesome": "*",
        "yiizh/yii2-imperavi-redactor": "*",
        "yiizh/yii2-cdn": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-authclient": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-jui": "*",
        "yii2tech/sitemap": "*",
        "vlucas/phpdotenv": "*",
        "kartik-v/yii2-widget-datetimepicker": "v1.4.2",
        "kartik-v/yii2-markdown": "*",
        "aliyuncs/oss-sdk-php": "~2.0",
        "bower-asset/slimscroll": "*",
        "bower-asset/clipboard": "*",
        "zendframework/zend-feed": "*"
    },
    "require-dev": {
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-debug": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-vcs-driver-options": {
            "github-no-api": true
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
